<p>
  <strong><%= t('.title') %></strong>
  <%= @test.title %>
</p>

<p>
  <strong><%= t('.level') %></strong>
  <%= @test.level %>
</p>

<p>
  <strong><%= t('.timer') %></strong>
  <%= @test.timer %>
</p>

<table>
  <tr>
    <th><%= t('.questions') %></th>
    <th><%= t('.actions') %></th>
  </tr>

  <%= render partial: 'admin/questions/question', collection: @test.questions %>

</table>

<p>
  <%= link_to t('.question'), new_admin_test_question_path(@test) %>

  <%= link_to t('.edit'), edit_admin_test_path(@test) %> |
 
  <%= link_to t('.back'), admin_tests_path %>
</p>
